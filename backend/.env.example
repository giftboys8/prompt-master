# Django安全设置
DJANGO_SETTINGS_MODULE=config.settings.dev
DJANGO_SECRET_KEY=your-django-secret-key-here  # Django系统加密密钥，用于会话等安全功能
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# JWT认证设置
JWT_SECRET_KEY=your-jwt-secret-key-here  # JWT token签名密钥，用于用户认证
JWT_ACCESS_TOKEN_LIFETIME=60  # 访问令牌有效期（分钟）
JWT_REFRESH_TOKEN_LIFETIME=1440  # 刷新令牌有效期（分钟）

# 数据库设置
POSTGRES_DB=promptmaster
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis设置
REDIS_URL=redis://localhost:6379/0

# Dify API设置
DIFY_API_KEY=your-dify-api-key-here  # Dify API调用密钥，用于访问Dify服务
DIFY_API_BASE_URL=https://api.dify.ai/v1  # Dify API基础URL